process {
  withLabel: download {
    publishDir {
      path = "${params.outdir}/download"
      mode = "${params.publishmode}"
    }
  }
  withLabel: json {
    publishDir {
      path = "${params.outdir}/JSON"
      mode = "${params.publishmode}"
    }
  }
  withLabel: archive {
    publishDir {
      path = "${params.outdir}/archive"
      mode = "${params.publishmode}"
    }
  }
  withLabel: tsv {
    publishDir {
      path = "${params.outdir}/tsv"
      mode = "${params.publishmode}"
    }
  }
}
